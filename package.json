{
  "name": "notes-pocketbase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:dev": "npm run generate-pb-types && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-pb-types": "dotenv -- npx pocketbase-typegen --url \"$VITE_POCKETBASE_URL\" --email $POCKETBASE_ADMIN_EMAIL --password \"$POCKETBASE_ADMIN_PASSWORD\" --out \"$POCKETBASE_TYPES_OUTPUT\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "pocketbase": "^0.26.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "pocketbase-typegen": "^1.3.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}